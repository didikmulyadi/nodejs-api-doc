{
  "name": "@didik-mulyadi/nodejs-api-doc",
  "version": "1.2.0",
  "description": "A nodejs library to generate the API Docs with multiple UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/didikmulyadi/nodejs-api-doc.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/node": "^20.1.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.4"
  },
  "homepage": "https://github.com/didikmulyadi/nodejs-api-doc",
  "bugs": "https://github.com/didikmulyadi/nodejs-api-doc/issues",
  "author": {
    "name": "Didik Mulyadi",
    "email": "didikmulyadi12@gmail.com",
    "url": "https://www.linkedin.com/in/didikmulyadi"
  },
  "keywords": [
    "api-docs",
    "api-documentation",
    "swagger",
    "redoc",
    "stoplight",
    "nodejs",
    "nestjs-api-doc",
    "nodejs-api-doc"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
